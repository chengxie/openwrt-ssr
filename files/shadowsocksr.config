
config general
	option startup_delay '0'
	list main_server 'nil'
	option udp_relay_server 'nil'
	option local_port '1234'
	option monitor_enable '1'
	option enable_switch '0'
		
config servers
	option auth_enable '0'
	option fast_open '0'
	option server '127.0.0.1'
	option server_port '8388'
	option password 'hello!!'
	option timeout '60'
	option encrypt_method 'rc4-md5'
	option protocol 'origin'
	option obfs 'plain'
	option obfs_param ''

config access_control
	option self_proxy '1'
	option wan_bp_list '/etc/shadowsocksr/china_chnroute.list'
